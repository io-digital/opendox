{
    "name": "io-digital/opendox",
    "description": "OpenApi(Swagger) 3.0 package for Laravel 9 with REDOC UI and SwaggerUI 3",
    "authors": [
        {
            "name": "Noitran.Black",
            "email": "noitran.black@gmail.com"
        },
        {
            "name": "Gareth Nicholson",
            "email": "gareth@io.co.za"
        }
    ],
    "keywords": [
        "api",
        "openapi",
        "swagger",
        "swagger-ui",
        "laravel",
        "lumen",
        "rest",
        "documentation",
        "redoc",
        "redoc-ui"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/routing": "^8.0|^9.0",
        "symfony/yaml": "^5.0|^6.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.6",
        "illuminate/console": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0",
        "laravel/lumen-framework": "^8.0|^9.0",
        "orchestra/testbench": "^7.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^5.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IoDigital\\Opendox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IoDigital\\Opendox\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IoDigital\\Opendox\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
